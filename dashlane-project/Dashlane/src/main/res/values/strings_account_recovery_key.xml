<resources>
    
    <string name="account_recovery_key_setting_title">Recovery key</string>
    
    <string name="account_recovery_key_setting_description_off">Off</string>
    
    <string name="account_recovery_key_setting_description_on">On</string>
    
    <string name="account_recovery_key_detailed_setting_title">Recovery key</string>
    
    <string name="account_recovery_key_detailed_setting_description">Your recovery key lets you access your vault if you forget your Master Password.</string>
    
    <string name="account_recovery_key_detailed_disable_dialog_title">Are you sure you want to turn off this recovery method?</string>
    
    <string name="account_recovery_key_detailed_disable_dialog_description">If you turn this off, your existing recovery key will no longer be valid.</string>
    
    <string name="account_recovery_key_detailed_disable_dialog_positive_button">Yes, turn off</string>
    
    <string name="account_recovery_key_detailed_disable_dialog_negative_button">Cancel</string>

    
    <string name="account_recovery_key_activation_intro_title">Your recovery key is a spare key to your vault</string>
    
    <string name="account_recovery_key_activation_intro_description">This is a single-use spare key to your vault. If you need to, you can use it to get back into your account and reset your Master Password.</string>
    
    <string name="account_recovery_key_activation_intro_button">Generate key</string>

    
    <string name="account_recovery_key_generate_title">Store this somewhere safe</string>
    
    <string name="account_recovery_key_generate_description_1">Your recovery key is one of the few ways you can access your Dashlane vault if you forget your Master Password.</string>
    
    <string name="account_recovery_key_generate_description_2">You may want to write it down and keep it with other important documents.</string>
    
    <string name="account_recovery_key_generate_button">Continue</string>

    
    <string name="account_recovery_key_confirm_title">Confirm your recovery key</string>
    
    <string name="account_recovery_key_confirm_description">If you didn’t make a copy of your key in the last step, go back and do that now.</string>
    
    <string name="account_recovery_key_confirm_label">Recovery Key</string>
    
    <string name="account_recovery_key_confirm_button">Confirm</string>
    
    <string name="account_recovery_key_confirm_error">This doesn’t look right. Check the key and try again.</string>

    
    <string name="account_recovery_key_success_title">Your recovery key is ready if you need it!</string>
    
    <string name="account_recovery_key_success_button">Done</string>

    
    <string name="login_account_recovery_key_enter_step">%1$d of %2$d</string>
    
    <string name="login_account_recovery_key_enter_title">Enter your recovery key</string>
    
    <string name="login_account_recovery_key_enter_description">Enter your single-use recovery key to recover access to your account and change your Master Password.</string>
    
    <string name="login_account_recovery_key_enter_label">Recovery Key</string>
    
    <string name="login_account_recovery_key_confirm_error">This doesn’t look right. Check the key and try again.</string>
    
    <string name="login_account_recovery_key_enter_button">Next</string>
    
    <string name="login_account_recovery_key_enter_success_title">You successfully recovered access to your account!</string>

