<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Dashlane.Login" parent="Theme.Dashlane.Modal" />

    <style name="Theme.Dashlane.Login.TranslucentWindow">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="Theme.Dashlane.Login.Dialog" parent="Theme.Dashlane.Login.TranslucentWindow">
        <item name="android:windowIsFloating">true</item>
        <item name="colorBiometricText">@color/text_inverse_catchy</item>
    </style>

    <style name="Theme.Dashlane.Login.Compose" parent="Theme.Dashlane.Login.TranslucentWindow">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

</resources>