<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >

    <com.dashlane.ui.widgets.view.ImportMethodItem
        android:id="@+id/csv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:methodicon="@drawable/ic_import_methods_csv"
        app:methodname="@string/import_methods_name_csv"
        />

    <com.dashlane.ui.widgets.view.ImportMethodItem
        android:id="@+id/computer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:methodicon="@drawable/ic_import_methods_m2d"
        app:methodname="@string/import_methods_name_m2d"
        />

    <com.dashlane.ui.widgets.view.ImportMethodItem
        android:id="@+id/backup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:methodicon="@drawable/ic_import_methods_dashlane_secure_archive"
        app:methodname="@string/import_methods_name_dashlane_secure_archive"
        />

    <com.dashlane.ui.widgets.view.ImportMethodItem
        android:id="@+id/password_manager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:methodicon="@drawable/ic_import_methods_competitor"
        app:methodname="@string/import_methods_name_competitor"
        />

</LinearLayout>